<!-- jukebox queue begins -->
<br>

<div id="playlist_border">
  <ul id="playlist">


    {{#songs}}
    <li id='remove_{{id}}'>
    <button class="video_name play btn btn-default btn-xs" id="play_{{video_id}}" data-title='{{video_title}}' data-id='{{video_id}}'>{{video_title}}</button> <span id="user_{{user_id}}">(user_{{user_id}})</span> <button class='remove btn btn-default btn-xs' data-id='{{id}}'>x</button>
</li>
    {{/songs}}


  </ul>
</div>
<!-- jukebox queue ends -->

<!-- add songs here -->
<br>
<div id="user_add_section">
  <form name="input" action="/songs">
    artist:
    <input type="text" id="artist_field">
    title:
    <input type="text" id="title_field">
    <button class="btn btn-default btn-sm" id="add_to_queue">Add to Queue</button>
  </form>
</div>
<br><br><br>
<!-- add songs ends -->
